<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".playWithPlayers">

    <LinearLayout
        android:layout_width="411dp"
        android:layout_height="370dp"
        android:background="#0f48a3"
        android:orientation="vertical"
        android:weightSum="10"
        android:padding="2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Royal Flush"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Straight Flush"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:padding="5dp"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Four of a Kind"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Full House"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Flush"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Straight"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Three of a Kind"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="Two Pair"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="One Pair"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_weight="1"
            android:background="@drawable/hand_ranking_background"
            android:text="High Card"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="23dp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>